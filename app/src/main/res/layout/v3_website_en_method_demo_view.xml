<?xml version="1.0" encoding="utf-8"?>
<com.kingsoft.shiyou.omnisdk.demo.v3.common.view.method.WebsiteEnMethodDemoView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/OmniDemoViewStyle"
    android:importantForAutofill="no"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:id="@+id/v3_website_method_demo_view_title_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            style="@style/OmniDemoAppFunctionTitleStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="官网渠道独有功能方法接口" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="1dp"
            android:background="@android:color/darker_gray" />
    </LinearLayout>

    <Button
        android:id="@+id/v3_website_method_demo_view_query_inapp_skus_btn"
        style="@style/OmniDemoAppButtonStyle"
        android:layout_width="match_parent"
        android:layout_below="@id/v3_website_method_demo_view_title_layout"
        android:layout_marginTop="10dp"
        android:text="获取本地化普通商品" />
    <Button
        android:id="@+id/v3_website_method_demo_view_query_subs_skus_btn"
        style="@style/OmniDemoAppButtonStyle"
        android:layout_width="match_parent"
        android:layout_below="@id/v3_website_method_demo_view_query_inapp_skus_btn"
        android:layout_marginTop="10dp"
        android:text="获取本地化订阅商品" />
</com.kingsoft.shiyou.omnisdk.demo.v3.common.view.method.WebsiteEnMethodDemoView>